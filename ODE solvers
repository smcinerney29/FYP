f = @(t,a) [(.6-(1/2)*.45*(1+tanh((288-280)/24))-a)];
[t,a]=ode45(f,[0 500],.28);
plot(t,a);


f = @(t,T) [((240-.615*(5.67*10^(-8))*T^4))/10^7];
[t,T]=ode45(f,[0 1000000000], 280);
plot(t,T)

f = @(t,p) [9.81*28.8*10^(-3)*(-92229473.6479*(2.2*10^(-3)*(p/(.35*10^(-3))^.3))+3.3*10^11)/((5.1*10^14)*44*10^(-3))];
